  const dispatch = useDispatch()\n  const nameMapping = useMemo(() => getNameMapping(units), [units])\n\n  useEffect(() => {\n    dispatch(getComparison({ units: units }))\n  }, [dispatch, units, target])\n\n  return (\n    <div>\n      <TargetSummary />\n      <CollapsibleCard title=\"Average Damage Table\">\n        {pending ? (\n          <TableSkeleton rows={units.length + 1} columns={3} dense />\n        ) : (\n          <ComparisonTable nameMapping={nameMapping} results={results} />\n        )}\n      </CollapsibleCard>\n      <CollapsibleCard title=\"Graphs\">\n        <ComparisonGraphs nameMapping={nameMapping} results={results} pending={pending} />\n      </CollapsibleCard>\n    </div>\n  )\n}\n\nexport default React.memo(Stats)\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Target/Target.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Units/Units.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/hooks/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/LeftNavigation/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Units/UnitCard.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/LeftNavigation/LeftNavigation.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/hooks/useIsMobile.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/utils/routes.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/selectors/unitsSelectors.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/LeftNavigation/ListItemLink.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-sta