/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/ModifierList/ModifierInput.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/slices/unitForm.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/WeaponProfileInfo/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/WeaponProfileInfo/WeaponProfileInfo.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/WeaponProfileInfo/StyledTableCell.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/UnitDialog/UnitDialog.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/UnitDialog/DialogContent.tsx",["403"],"import {\n  TextField,\n  DialogContent as MuiDialogContent,\n  makeStyles,\n  Theme,\n  useTheme,\n  Button,\n  Typography,\n} from '@material-ui/core'\nimport { Add } from '@material-ui/icons'\nimport WeaponProfileInfo from 'components/WeaponProfileInfo'\nimport WeaponProfileDialog, { openProfileDialog } from 'features/Forms/WeaponProfileDialog'\nimport React, { useCallback, useEffect } from 'react'\nimport { useDispatch } from 'react-redux'\nimport { useHistory } from 'react-router-dom'\nimport { unitFormStore } from 'store/slices'\nimport { UnitFormData, UnitFormErrors } from 'types/store/unitForm'\nimport { WeaponProfileParams } from 'types/store/units'\nimport ProfileControls from './ProfileControls'\n\nconst DEFAULT_PROFILE: WeaponProfileParams = {\n  numModels: 10,\n  attacks: 1,\n  toHit: 4,\n  toWound: 4,\n  rend: 0,\n  damage: 1,\n  modifiers: [],\n}\n\nconst useStyles = makeStyles((theme: Theme) => ({\n  nameField: {\n    paddingBottom: theme.spacing(3),\n  },\n  content: {\n    [theme.breakpoints.down('sm')]: {\n      padding: theme.spacing(1, 2),\n    },\n  },\n}))\n\ninterface DialogContentProps {\n  unitId: string\n  data: UnitFormData\n  errors?: UnitFormErrors\n}\n\nconst DialogContent = ({ unitId, data, errors }: DialogContentProps) => {\n  const dispatch = useDispatch()\n  const classes = useStyles()\n  const theme = useTheme()\n  const history = useHistory()\n\n  const handleNameChange = useCallback(\n    (event: React.ChangeEvent<HTMLInputElement>) => {\n      dispatch(unitFormStore.actions.editData({ key: 'name', value: event.target.value }))\n    },\n    [dispatch]\n  )\n\n  const handleProfileClicked = (id: string) => () => {\n    openProfileDialog(history, unitId, id)\n  }\n\n  const handleProfileEnabledChanged = useCallback(\n    (id: string) => (newValue: boolean) => {\n      dispatch(unitFormStore.actions.editWeaponProfile({ id, newProfile: { disabled: !newValue } }))\n    },\n    [dispatch]\n  )\n\n  const handleAddProfile = useCallback(() => {\n    dispatch(unitFormStore.actions.addWeaponProfile({ weaponProfile: DEFAULT_PROFILE }))\n  }, [dispatch])\n\n  return (\n    <>\n      <MuiDialogContent classes={{ root: classes.content }}>\n        <TextField\n          label=\"Unit Name\"\n          fullWidth\n          variant=\"outlined\"\n          className={classes.nameField}\n          value={data.name}\n          onChange={handleNameChange}\n          error={!!errors?.name}\n          helperText={errors?.name}\n        />\n        <Typography>Weapon Profiles:</Typography>\n        <div style={{ paddingBottom: theme.spacing(1) }} />\n        {data.weaponProfiles.map(profile => (\n          <WeaponProfileInfo\n            profile={profile}\n            key={profile.id}\n            onClick={handleProfileClicked(profile.id)}\n            onEnabledChanged={handleProfileEnabledChanged(profile.id)}\n            controls={<ProfileControls unitId={unitId} profile={profile} />}\n          />\n        ))}\n        <Button variant=\"contained\" fullWidth startIcon={<Add />} onClick={handleAddProfile}>\n          Add Profile\n        </Button>\n      </MuiDialogContent>\n      <WeaponProfileDialog />\n    </>\n  )\n}\n\nexport default React.memo(DialogContent)\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/UnitDialog/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/WeaponProfileDialog/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/WeaponProfileDialog/WeaponProfileDialog.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/WeaponProfileDialog/ProfileContent.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/WeaponProfileDialog/CharacteristicField.tsx",["404"],"import { TextField, Grid, InputAdornment } from '@material-ui/core'\nimport React, { useCallback, useMemo } from 'react'\nimport { useDispatch } from 'react-redux'\nimport { profileFormStore } from 'store/slices'\nimport { ProfileFormCharacteristicErrors, ProfileFormData, ProfileFormErrors } from 'types/store/profileForm'\n\ninterface FieldConfig {\n  label: string\n  type: 'number' | 'dice' | 'roll'\n  startAdornment?: string\n  endAdornment?: string\n}\n\ntype FieldConfigLookup = {\n  [key in keyof Omit<ProfileFormData, 'modifiers'>]: FieldConfig\n}\n\nconst fieldConfigLookup: FieldConfigLookup = {\n  numModels: { label: '# Models', type: 'number' },\n  attacks: { label: 'Attacks', type: 'dice' },\n  toHit: { label: 'To Hit', type: 'roll', endAdornment: '+' },\n  toWound: { label: 'To Wound', type: 'roll', endAdornment: '+' },\n  rend: { label: 'Rend', type: 'number', startAdornment: '-' },\n  damage: { label: 'Damage', type: 'dice' },\n}\n\ninterface CharacteristicFieldProps {\n  data: ProfileFormData\n  characteristic: keyof Omit<ProfileFormData, 'modifiers' | 'name' | 'disabled'>\n  errors?: ProfileFormCharacteristicErrors\n}\n\nconst CharacteristicField = ({ data, characteristic, errors }: CharacteristicFieldProps) => {\n  const dispatch = useDispatch()\n\n  const config = useMemo(() => fieldConfigLookup[characteristic], [characteristic])\n  const error = useMemo(() => (errors ? errors[characteristic] : undefined), [characteristic, errors])\n\n  const handleChange = useCallback(\n    (event: React.ChangeEvent<HTMLInputElement>) => {\n      let value: string | number = event.target.value\n      if (config.type === 'number') value = value !== '' ? Number(value) : ''\n      dispatch(profileFormStore.actions.editData({ key: characteristic, value }))\n    },\n    [dispatch, config, characteristic]\n  )\n\n  return (\n    <Grid item xs={4} sm={4} md={12}>\n      <TextField\n        fullWidth\n        variant=\"outlined\"\n        label={config.label}\n        type={config.type === 'number' ? 'number' : undefined}\n        name={characteristic}\n        value={data[characteristic]}\n        onChange={handleChange}\n        InputProps={{\n          startAdornment: config.startAdornment && (\n            <InputAdornment position=\"start\">{config.startAdornment}</InputAdornment>\n          ),\n          endAdornment: config.endAdornment && (\n            <InputAdornment position=\"end\">{config.endAdornment}</InputAdornment>\n          ),\n        }}\n        error={!!error}\n        helperText={error}\n      />\n    </Grid>\n  )\n}\n\nexport default React.memo(CharacteristicField)\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Forms/UnitDialog/ProfileControls.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/WeaponProfileInfo/ModifierSummary.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/ModifierDescription/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/ModifierDescription/ModifierDescription.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/api/comparison.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Stats/ComparisonTable.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Stats/graphs/ComparisonBarGraph.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/About/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/About/About.tsx",["405"],"import { Link, makeStyles, Paper, Theme, Typography } from '@material-ui/core'\nimport React from 'react'\nimport { EXTERNAL_LINKS } from 'utils/routes'\nimport AboutLink from './AboutLink'\nimport Section from './Section'\n\nconst useStyles = makeStyles((theme: Theme) => ({\n  root: {\n    maxWidth: 900,\n    margin: '10px auto',\n    marginTop: 10,\n  },\n  about: { margin: theme.spacing(0, 1) },\n  header: { marginBottom: theme.spacing(2) },\n  content: {\n    padding: theme.spacing(2, 6),\n    [theme.breakpoints.down('sm')]: {\n      padding: theme.spacing(2, 4),\n    },\n  },\n}))\n\nconst About = () => {\n  const classes = useStyles()\n\n  return (\n    <div className={classes.root}>\n      <Paper className={classes.about}>\n        <div className={classes.content}>\n          <Typography component=\"h2\" variant=\"h5\" align=\"center\" className={classes.header}>\n            AoS Statshammer\n          </Typography>\n          <Section title=\"Overview\">\n            A tool for calculating and comparing damage outputs for <strong>Warhammer Age of Sigmar </strong>\n            units (also referred to as Mathhammer). If you are interested in Warcry calculations, see the\n            sister-project <AboutLink href={EXTERNAL_LINKS.WARCRY_STATSHAMMER}>Warcry-Statshammer</AboutLink>.\n          </Section>\n          <Section title=\"Average Damage Comparison\">\n            Gather the average damage that each unit is expected to do against each possible save. This\n            average is a calculated value (not simulated).\n          </Section>\n          <Section title=\"Profile Modifiers\">\n            Add various modifiers to your weapon profiles in order to replicate the vast array of unit\n            rules/abilities that exist in the rules. Some examples are:\n            <ul>\n              <li>\n                <strong>Reroll Ones: </strong>\n                Reroll Ones for <i>to hit</i>\n              </li>\n              <li>\n                <strong>Mortal Wounds: </strong>\n                <i>Unmodified</i> rolls of <i>6</i>+ for <i>to hit</i> result in <i>1</i> mortal wounds\n              </li>\n              <li>\n                <strong>Leader Bonus: </strong>\n                Add <i>2</i> <i>attacks</i> to the leader of this unit (<i>1</i> leader)\n              </li>\n              <li>And many more</li>\n            </ul>\n          </Section>\n          <Section title=\"Target Modifiers\">\n            Add various modifiers to the target to which stats are generated against (the opposing unit).\n            Useful to replicate when where you are evaluating against a scenario where the opposing unit has\n            certain abilities. Some examples are:\n            <ul>\n              <li>\n                <strong>Target Reroll: </strong>\n                Reroll Save Rolls\n              </li>\n              <li>\n                <strong>Target Ethereal: </strong>\n                Ignore modifiers to save (positive or negative) when making save rolls\n              </li>\n              <li>\n                <strong>Target Feel No Pain: </strong>\n                Ignore wounds and mortal wounds on a roll of <i>6</i>+\n              </li>\n              <li>And many more</li>\n            </ul>\n          </Section>\n          <Section title=\"Simulations\">\n            Running a large number of simuled attacks to gather more detailed information on the performance\n            of the given units. The data points gathered include:\n            <ul>\n              <li>Cumulative Probabilies</li>\n              <li>Discrete Probabilies</li>\n              <li>Sample Metrics (mean, max, variance)</li>\n            </ul>\n            The number of simulations is customizable from the simulations screen.\n          </Section>\n          <Section title=\"Import / Export\">\n            There are various importing / exporting functionalities to make reproducing or saving results\n            easier.\n            <ul>\n              <li>Import / Export your units</li>\n              <li>\n                Export a PDF containing the generated information (unit data, comparisons, simulations, etc)\n              </li>\n            </ul>\n          </Section>\n          <Section title=\"Notes\">\n            This is a free community tool built by Damon Hook (<i>NoMaDhOoK</i>).\n            <br />\n            Keep an eye on the <AboutLink href={EXTERNAL_LINKS.RELEASES}>Github Releases</AboutLink>, or the\n            <AboutLink href={EXTERNAL_LINKS.REDDIT}>Reddit page</AboutLink> to get notified of new additions.\n            <br />\n            <br />\n            <strong>Remember:</strong> There is more to a unit than just its damage output. This is a tool,\n            not a definitive answer of which unit is better <br />\n          </Section>\n        </div>\n      </Paper>\n    </div>\n  )\n}\n\nexport default About\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Stats/ComparisonGraphs.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/hooks/useCurrentRoute.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/About/Section.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/pack