s-statshammer/packages/client/src/components/Skeletons/pages/UnitsSkeleton.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/Menu/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/Menu/Menu.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/LeftNavigation/ListItemLink.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/index.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/configureStore.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/slices/index.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/slices/modifiers.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/slices/units.ts",["200"],"import { createSlice, PayloadAction } from '@reduxjs/toolkit'\nimport UnitsStore, { UnitParams, WeaponProfile, WeaponProfileParams } from 'types/store/units'\nimport { nanoid } from 'nanoid'\n\nconst INITIAL_STATE: UnitsStore = {\n  items: [],\n}\n\nconst getDefaultUnit = (name: string = 'Unit'): UnitParams => ({\n  name: name,\n  weaponProfiles: [getDefaultWeaponProfile()],\n})\n\nconst getDefaultWeaponProfile = (): WeaponProfileParams => ({\n  numModels: 10,\n  attacks: 1,\n  toHit: 4,\n  toWound: 4,\n  rend: 0,\n  damage: 1,\n  modifiers: [],\n})\n\nexport default createSlice({\n  name: 'units',\n  initialState: INITIAL_STATE,\n  reducers: {\n    addUnit(state: UnitsStore, action: PayloadAction<{ unit?: UnitParams }>) {\n      const unit = action.payload.unit ?? getDefaultUnit(`Unit ${state.items.length + 1}`)\n      state.items.push({\n        ...unit,\n        id: nanoid(),\n        weaponProfiles: unit.weaponProfiles.map(profile => ({ ...profile, id: nanoid() })),\n      })\n    },\n    editUnitName(state: UnitsStore, action: PayloadAction<{ id: string; name: string }>) {\n      const { id, name } = action.payload\n      const unit = state.items.find(u => u.id === id)\n      if (unit) unit.name = name\n    },\n    deleteUnit(state: UnitsStore, action: PayloadAction<{ id: string }>) {\n      const { id } = action.payload\n      state.items = state.items.filter(u => u.id !== id)\n    },\n    addWeaponProfile(\n      state: UnitsStore,\n      action: PayloadAction<{ unitId: string; weaponProfile?: WeaponProfileParams }>\n    ) {\n      const { unitId } = action.payload\n      const unit = state.items.find(u => u.id === unitId)\n      if (unit) {\n        const weaponProfile = action.payload.weaponProfile ?? getDefaultWeaponProfile()\n        unit.weaponProfiles.push({ ...weaponProfile, id: nanoid() })\n      }\n    },\n    editWeaponProfile(\n      state: UnitsStore,\n      action: PayloadAction<{ unitId: string; id: string; newProfile: Partial<WeaponProfileParams> }>\n    ) {\n      const { unitId, id, newProfile } = action.payload\n      const unit = state.items.find(u => u.id === unitId)\n      let index = unit ? unit.weaponProfiles.findIndex(p => p.id === id) : -1\n      if (unit && index !== -1) {\n        const weaponProfile = unit.weaponProfiles[index]\n        unit.weaponProfiles[index] = { ...weaponProfile, ...newProfile, id: weaponProfile.id }\n      }\n    },\n    deleteWeaponProfile(state: UnitsStore, action: PayloadAction<{ unitId: string; id: string }>) {\n      const { unitId, id } = action.payload\n      const unit = state.items.find(u => u.id === unitId)\n      if (unit) {\n        unit.weaponProfiles = unit.weaponProfiles.filter(profile => profile.id !== id)\n      }\n    },\n  },\n})\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/slices/comparison.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/selectors/unitsSelectors.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/App/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/App/App.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Home/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Home/Home.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Home/TabPanel.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Home/stats/StatsTab.tsx",["201"],"import CollapsibleCard from 'components/CollapsibleCard'\nimport React from 'react'\nimport StatsSkeleton from 'components/Skeletons/pages/StatsSkeleton'\n\nconst StatsTab = () => {\n  return (\n    <div>\n      <StatsSkeleton />\n    </div>\n  )\n}\n\nexport default StatsTab\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Units/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Units/Units.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Units/UnitCard.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/Units/WeaponProfileCard.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/WeaponProfileDialog/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/WeaponProfileDialog/WeaponProfileDialog.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/WeaponProfileDialog/CharacteristicField.tsx",["202"],"import { TextField, Grid, InputAdornment } from '@material-ui/core'\nimport React, { useCallback, useMemo } from 'react'\nimport { useDispatch } from 'react-redux'\nimport { profileFormStore, unitsStore } from 'store/slices'\nimport { ProfileFormData } from 'types/store/profileForm'\n\ninterface FieldConfig {\n  label: string\n  type: 'number' | 'dice' | 'roll'\n  startAdornment?: string\n  endAdornment?: string\n}\n\ntype FieldConfigLookup = {\n  [key in keyof Omit<ProfileFormData, 'modifiers'>]: FieldConfig\n}\n\nconst fieldConfigLookup: FieldConfigLookup = {\n  numModels: { label: '# Models', type: 'number' },\n  attacks: { label: 'Attacks', type: 'dice' },\n  toHit: { label: 'To Hit', type: 'roll', endAdornment: '+' },\n  toWound: { label: 'To Wound', type: 'roll', endAdornment: '+' },\n  rend: { label: 'Rend', type: 'number', startAdornment: '-' },\n  damage: { label: 'Damage', type: 'dice' },\n}\n\ninterface CharacteristicFieldProps {\n  data: ProfileFormData\n  characteristic: keyof Omit<ProfileFormData, 'modifiers'>\n}\n\nconst CharacteristicField = ({ data, characteristic }: CharacteristicFieldProps) => {\n  const dispatch = useDispatch()\n\n  const config = useMemo(() => fieldConfigLookup[characteristic], [characteristic])\n\n  const handleChange = useCallback(\n    (event: React.ChangeEvent<HTMLInputElement>) => {\n      let value: string | number = event.target.value\n      if (config.type === 'number') value = Number(value)\n      dispatch(profileFormStore.actions.editData({ key: characteristic, value }))\n    },\n    [dispatch, config, characteristic]\n  )\n\n  return (\n    <Grid item xs={4} sm={4} md={12}>\n      <TextField\n        fullWidth\n        variant=\"outlined\"\n        label={config.label}\n        type={config.type === 'number' ? 'number' : undefined}\n        name={characteristic}\n        value={data[characteristic]}\n        onChange={handleChange}\n        InputProps={{\n          startAdornment: config.startAdornment && (\n            <InputAdornment position=\"start\">{config.startAdornment}</InputAdornment>\n          ),\n          endAdornment: config.endAdornment && (\n            <InputAdornment position=\"end\">{config.endAdornment}</InputAdornment>\n          ),\n        }}\n      />\n    </Grid>\n  )\n}\n\nexport default CharacteristicField\n","/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/types/store/units.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/features/WeaponProfileDialog/ProfileContent.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/store/slices/profileForm.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/api/modifiers.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/api/helpers.ts",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/ModifierSelector/index.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/ModifierSelector/ModifierSelector.tsx",[],"/Users/damon.hook/Documents/personal/statshammer/aos-statshammer/packages/client/src/components/ModifierSelector/ModifierItems.tsx",[],{"ruleId":"203","replacedBy":"204"},{"ruleId":"205","replacedBy":"206"},{"ruleId":"207","severity":1,"message":"208","line":2,"column":34,"nodeType":"209","messageId":"210","endLine":2,"endColumn":47},{"ruleId":"207","severity":1,"message":"211","line":1,"column":8,"nodeType":"209","messageId":"210","endLine":1,"endColumn":23},{"ruleId":"207","severity":1,"message":"212","line":4,"column":28,"nodeType":"209","messageId":"210","endLine":4,"endColumn":38},"no-native-reassign",["213"],"no-negated-in-lhs",["214"],"@typescript-eslint/no-unused-vars","'WeaponProfile' is defined but never used.","Identifier","unusedVar","'CollapsibleCard' is defined but never used.","'unitsStore' is defined but never used.","no-global-assign","no-unsafe-negation"]